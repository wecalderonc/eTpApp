<p id="notice"><%= notice %></p>

<h1>Problemsolutions</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @problemsolutions.each do |problemsolution| %>
      <tr>
        <td><%= link_to 'Show', problemsolution %></td>
        <td><%= link_to 'Edit', edit_problemsolution_path(problemsolution) %></td>
        <td><%= link_to 'Destroy', problemsolution, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Problemsolution', new_problemsolution_path %>
